    "require": {
        "php": ">=8.2.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1"
    },
    "autoload": {
        "psr-4": { 
            "robertsaupe\\SystemInfo\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "8.2.0"
        },
        "platform-check": false,
        "sort-packages": true
    },
    "scripts": {
        "stan": "phpstan analyse -vvv",
        "test-check": "@php test/Check.php",
        "test-info": "@php test/Info.php",
        "test-os": "@php test/OS.php"
    }
}
